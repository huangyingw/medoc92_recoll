[remote "medoc92"]
    url = https://framagit.org/medoc92/recoll.git
    fetch = +refs/heads/*:refs/remotes/medoc92/*
[remote "origin"]
    url = git@github.com:huangyingw/medoc92_recoll.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = medoc92
    target = medoc92/master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
    ignore = ./src/*.sh
[deploy]
	host = localhost
	path = ~/myproject/git/tools/medoc92/recoll
